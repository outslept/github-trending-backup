{
  "name": "github-trending-backup",
  "packageManager": "pnpm@10.6.3",
  "type": "module",
  "scripts": {
    "lint": "npx oxlint && npx eslint",
    "lint:fix": "npx oxlint && npx eslint --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "lint-staged"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.0.2",
    "@types/node": "^22.15.17",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^1.2.0",
    "lint-staged": "^16.1.2",
    "publint": "^0.3.12",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "oxlint",
      "eslint --fix"
    ]
  }
}
