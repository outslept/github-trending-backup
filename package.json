{
  "name": "github-trending-backup",
  "packageManager": "pnpm@10.6.3",
  "scripts": {
    "dev": "tsx index.ts",
    "start": "tsx cli.ts",
    "build": "tsdown index.ts cli.ts git-ops.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.3",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.11.1",
    "tsdown": "0.11.0-beta.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*.{js,ts,mjs}": "eslint --fix"
  }
}
