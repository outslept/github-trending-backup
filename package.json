{
  "name": "github-trending-backup",
  "packageManager": "pnpm@10.6.3",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "catalog:lint",
    "@types/node": "catalog:types",
    "eslint": "catalog:lint",
    "nano-staged": "catalog:script",
    "publint": "catalog:lint",
    "simple-git-hooks": "catalog:script",
    "typescript": "catalog:build"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
