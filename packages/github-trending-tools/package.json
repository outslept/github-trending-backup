{
  "name": "github-trending-tools",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.10.0",
  "description": "GitHub trending repositories scraper",
  "keywords": [
    "github",
    "trending",
    "scraper",
    "repositories"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsdown index.ts --format esm --clean --sourcemap",
    "start": "tsx index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "node-html-parser": "catalog:script"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "tsdown": "catalog:build",
    "tsx": "catalog:script"
  }
}
